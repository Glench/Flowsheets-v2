    "codemirror": "^5.24.2",
    "electron": "^1.4.15",
    "filbert": "^0.1.20",
    "flow-bin": "^0.38.0",
    "jquery": "^3.1.1",
    "less": "^2.7.2",
    "ps-node": "^0.1.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "underscore": "^1.8.3"
  }
}
